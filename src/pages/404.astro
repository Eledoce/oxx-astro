---
import Layout from '@/layouts/Layout.astro'
import url from '@/scripts/base'
import urlBase from '@/scripts/base'
---

<Layout title="404">
  <div>
    <a href=`${url}/`>
      404 <br /> No encontrado <br />
    </a>
  </div>
</Layout>

<style>
  div {
    @apply text-4xl h-screen border;
  }
  a {
    @apply text-5xl  h-[80vh] text-center font-Inter border flex font-bold justify-center items-center flex-col;
  }
</style>
